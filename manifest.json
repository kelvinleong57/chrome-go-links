{
	"manifest_version": 2,

	"name": "Go Links",
	"description": "This extension allows user to search using shortcuts",
	"version": "1.0",
	"omnibox": { "keyword" : "te" },
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html",
		"default_title": "Go Links"
	},
	"permissions": [
		"activeTab",
		"https://ajax.googleapis.com/"
	]
}
